{
    "name": "apps-hopes-front",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --base-href=/hopes/",
        "build": "ng build",
        "build:pre": "ng build --configuration=preproduction --base-href=/hopes/",
        "build:dev": "ng build --configuration=dev --base-href=/hopes/ --output-path=dist/apps-hopes-front-dev",
        "test": "ng test",
        "lint": "prettier --check ./src/**/**.{ts,html}",
        "stylelint": "stylelint **/*.scss",
        "reformat:ts": "prettier --write ./src/**/**.ts",
        "reformat:html": "prettier --write ./src/**/**.html",
        "reformat:scss": "stylelint --fix ./src/**/**.scss",
        "reformat:all": "stylelint --fix ./src/**/**.scss && prettier --write ./src/**/**.html && prettier --write ./src/**/**.ts",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^9.1.1",
        "@angular/cdk": "^9.2.4",
        "@angular/common": "~9.1.0",
        "@angular/compiler": "~9.1.0",
        "@angular/core": "~9.1.0",
        "@angular/forms": "~9.1.0",
        "@angular/localize": "^9.1.2",
        "@angular/platform-browser": "~9.1.0",
        "@angular/platform-browser-dynamic": "~9.1.0",
        "@angular/router": "~9.1.0",
        "@ng-bootstrap/ng-bootstrap": "^6.0.3",
        "@ngx-translate/core": "^12.1.2",
        "@ngx-translate/http-loader": "^4.0.0",
        "@swimlane/ngx-charts": "^14.0.0",
        "angular-feather": "^6.0.10",
        "angular-google-charts": "^1.1.4",
        "angular-tree-component": "^8.5.6",
        "bootstrap": "^4.4.1",
        "file-saver": "^2.0.2",
        "font-awesome": "^4.7.0",
        "jquery": "3.4.1",
        "lodash": "^4.17.20",
        "moment": "^2.26.0",
        "ng5-slider": "^1.2.4",
        "ngx-bootstrap-multiselect": "^2.1.0",
        "ngx-contextmenu": "^5.4.0",
        "ngx-toastr": "^12.0.1",
        "npm-force-resolutions": "0.0.3",
        "popper.js": "^1.16.1",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "xlsx": "^0.16.2",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.901.7",
        "@angular/cli": "~9.1.0",
        "@angular/compiler-cli": "~9.1.0",
        "@angular/language-service": "~9.1.0",
        "@types/file-saver": "^2.0.1",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "husky": "^4.2.5",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^5.0.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~3.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "minimist": "^1.2.5",
        "prettier": "^2.0.5",
        "protractor": "~5.4.3",
        "stylelint": "^13.3.3",
        "stylelint-config-standard": "^20.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.8.3"
    },
    "resolutions": {
        "minimist": "^1.2.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run stylelint"
        }
    }
}
